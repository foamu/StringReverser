package javaApp1;

import java.util.Arrays;

public class PersonNames {
	
	private String[] names = new String[] {
			"Adam", 
			"Fred",
			"Jessica",
			"Walker",
			"Francois"
			};

	public String[] getNames() {
		return names;
	}

	public void setNames(String[] names) {
		this.names = names;
	}

	@Override
	public String toString() {
		return Arrays.toString(names);
	}
}
